                                                                                                @Html.DisplayFor(item => Model.ProgramWorkouts[i].wName)
                                                                                                <br />

                                                                                                <div class="hr"></div>@Html.DisplayFor(item => Model.ProgramWorkouts[i].wArea)<br /> <a href="@Url.Action("WorkoutPage", "Excercises", new {wid = Model.ProgramWorkouts[i].wId, wnum = 1, Completed = false})" class="btn btn-default btn-block"><span>Start</span></a>
                                                                                            </div>
                                                                                        }
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <div class="col-lg-1 col-md-2 col-sm-4 col-xs-6  dayBox1">
                                                                                            @Html.DisplayFor(item => Model.ProgramWorkouts[i].wName)
                                                                                            <br/>

                                                                                            <div class="hr"></div>@Html.DisplayFor(item => Model.ProgramWorkouts[i].wArea)<br/> <a href="@Url.Action("WorkoutPage", "Excercises", new {wid = Model.ProgramWorkouts[i].wId, wnum = 1})" class="btn btn-default btn-block"><span>Start</span></a>
                                                                                        </div>
                                                                                    }



                                                                                }
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                                                    }

        }
        <div class="hr"></div>
    </div>


