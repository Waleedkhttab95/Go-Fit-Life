<body class="bccol">

    <div class="container" style="margin-top: 130px;">
        <div class="container">


            <div class="content1 InnerContainerWorkoutpage">
                <div class="row">

                    <!-- List of Category Excercises -->
        
                    <h2 class="mm">Please select an excercise to display: </h2>           
                    <ul class="list-group">
                       @for (int i = 1; i <= Model.ExcerciseItemCat.Length; i++)
                       {
                         
                            <a class="list-group-item txtListExc" href="@Url.Action("ExcCategory", "Excercises", new {BodyPartid = Model.BodyPartId ,  ExCatId = i})" >@Html.DisplayName(Model.ExcerciseItemCat[i-1].ExName)</a>
                        }
                    </ul>
                    
                    
                    
                    
                    <!-- List of Category Excercises -->
                </div>
            </div>
        </div>
    </div>
</body>
